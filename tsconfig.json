{
  "extends": "./tsconfig.path.json",
  "compilerOptions": {
    "strict": true,
    "alwaysStrict": true,
    "rootDir": "./",
    "baseUrl": "src/",
    "outDir": "dist/",
    "newLine": "LF",
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "ES2015",
      "ES2018",
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "types": [
      "node",
      "vite/client"
    ],
    "noEmit": true,
    "useUnknownInCatchVariables": false,
    "noImplicitAny": false,
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules",
    "server",
    "**/*deprecate*"
  ],
  "include": [
    "next-env.d.ts",
    "src/*.ts",
    "src/*.tsx"
, "globals.d.ts"  ]
}